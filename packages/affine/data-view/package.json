      "./view-presets": {
        "import": "./dist/view-presets/index.js",
        "types": "./dist/view-presets/index.d.ts"
      },
      "./widget-presets": {
        "import": "./dist/widget-presets/index.js",
        "types": "./dist/widget-presets/index.d.ts"
      }
