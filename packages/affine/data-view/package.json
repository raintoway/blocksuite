    "@blocksuite/store": "workspace:*",
    "@blocksuite/affine-components": "workspace:*",
    "@blocksuite/icons": "^2.1.66",
    "@floating-ui/dom": "^1.6.10",
    "@lit-labs/preact-signals": "^1.0.2",
    "@lit/context": "^1.1.2",
    "@toeverything/theme": "^1.0.7",
    "lit": "^3.2.0",
    "date-fns": "^3.6.0",
    "sortablejs": "^1.15.2",
    "zod": "^3.23.8"
